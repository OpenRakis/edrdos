# Watcom linker file for BIOS.EXE (is further processed to DRBIOS.SYS)

system dos
option quiet
option nocaseexact
option map=bin\bios.map

name bin\bios
file bin\biosgrps
file bin\init
file bin\clock
file bin\console
file bin\disk
file bin\serpar
file bin\biosmsgs
file bin\biosinit
file bin\config
file bin\genercfg
file bin\nlsfunc
file bin\bdosldr
file bin\stacks
file bin\initmsgs
