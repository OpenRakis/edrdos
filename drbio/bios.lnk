# Watcom linker file for BIOS.EXE (is further processed to DRBIOS.SYS)

system dos
option quiet
option nocaseexact
option map=bin/bios.map
disable 1014

name bin/bios
file bin/biosgrps.obj
file bin/init.obj
file bin/clock.obj
file bin/console.obj
file bin/disk.obj
file bin/serpar.obj
file bin/biosmsgs.obj
file bin/biosinit.obj
file bin/config.obj
file bin/genercfg.obj
file bin/nlsfunc.obj
file bin/bdosldr.obj
file bin/stacks.obj
file bin/initmsgs.obj
